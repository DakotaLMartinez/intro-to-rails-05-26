<div class="container">
  <% if @error %>
    <div class="alert alert-danger" role="alert">
      <%= @error %>
    </div>
  <% end %>
  <%= bootstrap_form_for(@user, url: '/sessions') do |f| %>
    <%= f.email_field :email %>
    <%= f.password_field :password %>
    
    <%= f.submit "Log In" %>
  <% end %>
</div>